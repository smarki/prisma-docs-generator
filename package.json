  "dependencies": {
    "@prisma/dmmf": "^6.7.0",
    "@prisma/generator-helper": "^6.7.0",
    "@prisma/internals": "^6.7.0",
    "express": "^5.1.0",
    "indent-string": "^5.0.0",
